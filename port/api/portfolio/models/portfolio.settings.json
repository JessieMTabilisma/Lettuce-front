{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "portfolios",
  "info": {
    "name": "Portfolio"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "projectTitle": {
      "type": "string"
    },
    "projectThumbnail": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "projectDescription": {
      "type": "text"
    },
    "projectGallery": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "projectContent": {
      "type": "richtext"
    },
    "projectCategory": {
      "type": "enumeration",
      "enum": [
        "webApplication",
        "portfolio",
        "graphics",
        "mobileApplication",
        "maintenance"
      ]
    }
  }
}
